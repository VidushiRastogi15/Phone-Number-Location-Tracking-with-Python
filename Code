# Code
pip install phonenumbers

pip install geocoder

import phonenumbers
from phonenumbers import geocoder

phone_number1 = phonenumbers.parse("+917xxxxxx621")
phone_number2 = phonenumbers.parse("+918xxxxxx652")
phone_number3 = phonenumbers.parse("+919xxxxxx428")
phone_number4 = phonenumbers.parse("+916xxxxxx488")

# let's track your location 
print("\nPhone Number Location\n")
print(geocoder.description_for_number(phone_number1, "en"));
print(geocoder.description_for_number(phone_number2, "en"));
print(geocoder.description_for_number(phone_number3, "en"));
print(geocoder.description_for_number(phone_number4, "en"));
